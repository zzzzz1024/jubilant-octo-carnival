{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "编译代码并调试酒馆网页 (Chrome)",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "开始任务",
      "postDebugTask": "结束任务",
      "url": "http://localhost:6622",
      "disableNetworkCache": true,
      "internalConsoleOptions": "neverOpen",
      "cwd": "${workspaceFolder}",
      "timeout": 100000,
      "sourceMapPathOverrides": {
        "src://tavern_helper_template/*": "${workspaceFolder}/*",
        "webpack://tavern_helper_template/*": "${workspaceFolder}/*",
      },
      "skipFiles": [
        "**/jquery*.min.js",
        "**/node_modules/**"
      ],
    },
    {
      "name": "编译代码并调试酒馆网页 (Edge)",
